/* pages/service/service.wxss */
.root-wrapper{
  width: 100%;
  position:absolute;
}
.app_logo-wrapper{
  width: 100%;
  height: 350rpx;
  padding: 1px 0 1px 0;
  background-color: rgba(124, 124, 124, 0.2);
}
.app_logo{
  align-items: center;
  position:absolute;
}

.header-wrapper{
  background-color: rgba(255, 255, 255, 0.2);
  padding-bottom: 1px;
}

.switch_wrapper{
    margin-right: 0%;
    height: 40px;
    width: 30%;
    align-items: center;
    overflow: hidden;
}

.line{
  padding-top: 2px;
  padding-bottom: 2px;
  margin-left: 12px;
  color: #444444;
  font-size: 14px;
}

.body-wrapper{
  background-color: rgba(189, 189, 189, 0.2);
}

.list-wrapper{
  background-color: rgba(189, 189, 189, 0.2);
}
.list{
  padding: 5px 0 5px 0;
  margin-top: 1px;  
  padding-inline-start: 12px;
  background-color: rgb(255, 255, 255);
  color: rgb(29, 29, 28);
  display: flex;
  align-items: center;
  overflow: hidden;
}
.logo{
  background-color: red;
  height:36px;
  width:36px;
  border-radius: 4px;
}
.list_data{
  margin-left: 12px;
}